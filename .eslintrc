{
  "extends": "airbnb",
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "indent": [2, 4, {"SwitchCase": 1}],
    "react/prop-types": 0,
    "react/jsx-indent": ["error", 4],
    "react/jsx-indent-props": [2, 4],
    "linebreak-style": ["error", "windows"]
  }
}